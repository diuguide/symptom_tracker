<header class="jumbotron jumbotron-fluid bg-maroon">
  <div class="container">
    <h1 class="display-4">Welcome to Myriad!</h1>

    <h5>Please remember to fill out your daily check-in.</h5>

  </div>
</header>
<div class="container">

  <div class="row">
    <div class="col-6">
      <h1>Email: {{email}}</h1>
    </div>
  </div>

  <div class="row">
    <div class="col-2">
      <h6>Symptoms:</h6>
    </div>
    <div class="col-6">
      <div class="date-grid"></div>
    </div>
  </div>

  <div class="row" style="height: 260px;">
    <div class="col-2">
      <div class="symptom-grid"></div>
    </div>
    <div class="col-5">
      <div class="day-grid"></div>
    </div>
    <div class="col-3">
      <div class="calenderGrid">
        {{> grid/calender}}
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-12">
      <h2><a class="logout" href="/logout">Logout</a></h2>
    </div>
  </div>
</div>
<br>
<br>
{{!-- Create day modal to pop up when daysquare is clicked --}}

<div id="dayModal" class="modal">

  <!-- Modal content -->
  <div class="modal-content p-4">
    <span id="class" class="close">&times;</span>
    <p>{{> forms/symptoms}}</p>

    {{!-- <p>Rate your symptom from 1-10</p>
    <div class="rating-area"></div> --}}
  </div>

</div>

<!-- jQuery -->
<script src="https://code.jquery.com/jquery-3.5.1.min.js"
  integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
<!-- connect to js -->
<script src="/js/userhome.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.27.0/moment.min.js"></script>